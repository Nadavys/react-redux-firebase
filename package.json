{
  "name": "generated-firebase",
  "version": "0.0.1",
  "description": "Project generated by generator-react-firebase",
  "main": "index.js",
  "scripts": {
    "build": "craco build",
    "eject": "craco eject",
    "setup": "echo \"Generating client config...\" && env-cmd bin/generate-firebase-sdk-config.js --file ./config/default.json",
    "check-config": "if [ ! -f ./src/client-config.json ]; then npm run setup; else echo \"Client config already exists, running command...\"; fi;",
    "start": "npm run check-config && env-cmd craco start",
    "start:dist": "npm run build && firebase emulators:start --only hosting",
    "start:emulate": "cross-env NODE_APP_INSTANCE=emulators npm run start",
    "functions:start": "npm run start --prefix functions",
    "functions:watch": "npm run watch --prefix functions",
    "functions:build": "npm run build --prefix functions",
    "emulators": "firebase emulators:start --only firestore,database,pubsub",
    "emulators:hosting": "firebase emulators:start --only firestore,database,hosting",
    "emulators:all": "firebase emulators:start",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --single-quote --no-semi --trailing-comma none --write \"src/**/*.js\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/testuser/generated-firebase.git"
  },
  "engines": {
    "node": ">14.15.0"
  },
  "author": "testuser (https://github.com/testuser)",
  "license": "MIT",
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "firebase": "^8.7.1",
    "notistack": "^1.0.9",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-button": "^0.7.2",
    "react-redux": "^7.1.1",
    "react-redux-firebase": "^3.8.1",
    "react-hook-form": "^7.6.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.4",
    "redux-auth-wrapper": "^2.1.0",
    "redux-firestore": "^0.11.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@craco/craco": "6.2.0",
    "@sentry/cli": "^1.67.1",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "babel-eslint": "^10.1.0",
    "config": "^3.3.6",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsdoc": "^34.7.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^5.0.0",
    "firebase-ci": "^0.15.0",
    "firebase-tools": "^9.16.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.0.0",
    "minimist": "1.2.5",
    "prettier": "^2.3.0",
    "react-scripts": "^4.0.3",
    "typescript": "^4.2.4"
  },
  "eslintConfig": {
    "extends": "./eslintrc.js"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts}": [
      "eslint --fix"
    ]
  },
  "config": {
    "port": 3000
  }
}
